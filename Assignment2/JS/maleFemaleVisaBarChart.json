{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "title": "Male vs Female Migrants - Visa Type",
    "data": {
      "url": "https://raw.githubusercontent.com/PGrinEk/FIT3179/refs/heads/main/Assignment2/Data/genderVisaType.csv"
    },
    "mark": "bar",
    "encoding": {
    "x": {
      "field": "Visa",
      "type": "nominal",
      "axis": {"labelAngle": 0}
    },
    "y": {
      "field": "Count",
      "type": "quantitative",
      "title": "Number of Migrants"
    },
    "color": {
      "field": "Gender",
      "type": "nominal",
      "scale": {"domain": ["Male", "Female"], "range": ["#1f77b4", "#ff7f0e"]},
      "title": "Gender"
    },
    "column": {
      "field": "gender",
      "type": "nominal"
    }
}
}
